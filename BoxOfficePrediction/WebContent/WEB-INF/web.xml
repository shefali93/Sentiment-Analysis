<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:web="http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd" id="WebApp_ID" version="3.0">
  <servlet>
    <servlet-name>DisplayResult</servlet-name>
    <servlet-class>com.Servlet.Predict</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>CollectTweet</servlet-name>
    <servlet-class>com.Servlet.collectTweet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>Process</servlet-name>
    <servlet-class>com.Servlet.Processing</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>Insert</servlet-name>
    <servlet-class>com.Servlet.InsertMovie</servlet-class>
  </servlet>
  
  
  <servlet-mapping>
    <servlet-name>DisplayResult</servlet-name>
    <url-pattern>/result.do</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>CollectTweet</servlet-name>
    <url-pattern>/collection.do</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>Process</servlet-name>
    <url-pattern>/process.do</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>Insert</servlet-name>
    <url-pattern>/insert.do</url-pattern>
  </servlet-mapping>
</web-app>